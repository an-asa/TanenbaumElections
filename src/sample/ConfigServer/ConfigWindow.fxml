<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" maxHeight="100.0" maxWidth="475.0" prefHeight="100.0" prefWidth="475.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ConfigServer.ConfigWindowController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <VBox alignment="CENTER" prefHeight="200.0" spacing="10.0">
        <Label alignment="CENTER" text="Plik konfiguracyjny" />
        <Button fx:id="fileChooserButton" maxWidth="100.0" mnemonicParsing="false" onAction="#fileChooserButton_onClicked" text="Wybierz plik...">
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </Button>
    </VBox>
    <VBox alignment="CENTER" prefHeight="200.0" spacing="10.0">
        <Label text="Czas oczekiwania węzła [ms]" />
        <TextField fx:id="timeoutTextField" alignment="CENTER_RIGHT" text="1000" />
    </VBox>
    <Separator orientation="VERTICAL" prefHeight="200.0" />
    <Button fx:id="launchButton" mnemonicParsing="false" onAction="#launchButton_onClicked" prefHeight="50.0" prefWidth="100.0" text="Uruchom">
        <HBox.margin>
          <Insets />
        </HBox.margin>
    </Button>
</HBox>
